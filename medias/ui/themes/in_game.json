				"type":"Solid"
			},
			"cornerRadius": 2,
			"progressBar":
			{
				"fillColor": { "rgba": [ 0, 0, 0, 100 ] }
			}
		}
	},

	"equipableItemBox":
	{
		"background": { "type":"Transparent" },
		"fontSize": 17,
		
		"itemSlot":
		{
			"background": 
			{
				"colorStateList":
				{
					"normal":{"rgba":[0,0,0,100]}
