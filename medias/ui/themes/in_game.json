{
    "borderType": "Solid",
    "cornerRadius": 3.0,
	"drawDropShadow": false,
	"drawTextShadow": true,
	"fontSize": 20,
	"fontColor": { "rgba": [ 255, 255, 255, 255 ] },
	"font": { "path": "medias\\ui\\fonts\\clear-sans.medium.ttf" },
	"iconFont": { "path": "medias\\ui\\fonts\\entypo.ttf" },
	"borderColor": { "rgba": [ 100, 100, 100, 50 ] },
    "disabledTextColor": { "rgba": [ 217, 217, 217, 200 ] },
    "dropShadowColor": { "rgba": [ 180, 180, 180, 20 ] },
    "disabledColor": { "rgba": [ 150,  150,  150, 200 ] },
	"scale": 1,
	
	"background": 
	{
		"colorStateList":
		{
			"normal":{"rgba":[30,30,30,100]},
			"hovered":{"rgba":[50,50,50,100]},
			"clicked":{"rgba":[30,30,30,100]},
			"disabled":{"rgba":[0,255,0,100]},
			"focused":{"rgba":[203,71,0,100]},
			"selected":{"rgba":[203,71,0,100]}
		},
		"type":"Solid"
	},
		
	"progressBar":
	{
		"cornerRadius": 0,
		"borderType": "None",
		"background": { "type":"Transparent" }
	},
	
	"contextMenuStripItem":
	{
		"cornerRadius": 0
	},
	
	"pictureBox":
	{
		"background": { "type":"Transparent" }
	},
	
		
	"listBox":
	{
		"background": { "type":"Transparent" },
			
		"listBoxItem":
		{
			"background": 
			{
				"colorStateList":
				{
					"hovered":{"rgba":[203,50,50,160]}
				},
				"type":"Solid"
			}
		}
	},
	
	"itemBox":
	{
		"background": { "type":"Transparent" },
		"fontSize": 17,
		
		"itemSlot":
		{
			"background": 
			{
				"colorStateList":
				{
					"normal":{"rgba":[0,0,0,100]}
